                var obj = eval(msg);
                var a = obj["test"];
                for(var i=0;i<a.length;i++)
                {
                    for(var j=0;j<a[i].length;j++)
                    {
                        //alert("第"+i+"行 第"+j+"列 的值为:"+a[i][j]);
                        if(j==1){
                     	   a[i][j] = Number(a[i][j]);
                        }
                        }
                    }
                 chart2.series[0].setData(a);
            }
        });
    });
});
